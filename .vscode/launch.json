    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Benchmark",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--start-seed",
                "2",
                "--num-seeds",
                "1",
                "--workers",
                "0"
            ]
        },
        {
            "name": "Python: Main",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--wandb-project-name",
                "test_space",
                "--add-weight-decay",
                "--weight-decay",
                "1e-4"
            ]
        }
    ]
}